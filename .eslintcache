eck status code here and throw for errors so our catch will work.\n      .then(res => res.json())\n      .then(({ totalResults, articles }) => this.setState({ total: totalResults, list: articles, loading: false }))\n      .catch(() => this.setState({ loading: false, error: true }))\n  }\n\n  render() {\n    const { children } = this.props\n    return (\n      <div>\n        {children({\n          ...this.props,\n          ...this.state,\n        })}\n      </div>\n    )\n  }\n}\n\nclass ComplexList extends Component {\n  render() {\n    const { renderHeader, renderListItem, data, children } = this.props\n    return (\n      <div>\n        <div className=\"header\">{renderHeader(this.props)}</div>\n        <div className=\"main\">{data.map(item => renderListItem(item))}</div>\n        <div className=\"footer\">{children}</div>\n      </div>\n    )\n  }\n}\n\nconst renderListItem = ({ author, publishedAt, title, url }) => (\n  <div key={url}>\n    <h4 className=\"App-link\" href={url}>\n      {title} - {publishedAt.replace(/T.*$/, '')}\n    </h4>\n    <p>{author}</p>\n  </div>\n)\n\nexport default function LoadComponent() {\n  return (\n    <LoadContent url={`${URL}`}>\n      {({ loading, error, total, list, ...props }) => {\n        if (loading) return <span>Loading...</span>\n        if (error) return <span>Error loading</span>\n\n        console.log('----', total, list)\n        return (\n          <ComplexList\n            data={list}\n            renderHeader={() => <span>{loading ? 'Loading...' : 'Header Content'}</span>}\n            renderListItem={renderListItem}\n          >\n            We have {total} items\n          </ComplexList>\n        )\n      }}\n    </LoadContent>\n  )\n}\n","C:\\Users\\willi\\vscodes\\ms-frontend-framework\\src\\components\\index.js",[],"C:\\Users\\willi\\vscodes\\ms-frontend-framework\\src\\components\\home.js",[],"C:\\Users\\willi\\vscodes\\ms-frontend-framework\\src\\components\\about.js",[],"C:\\Users\\willi\\vscodes\\ms-fron